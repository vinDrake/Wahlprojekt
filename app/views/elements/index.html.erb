

<h1>Elemente</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elements.each do |element| %>
      <tr>
        <td><%= element.challenge.name %></td>
        <td><%= element.question.problem %></td>
        <td><%= link_to (image_tag "lupe_318-1787.jpg"), element %></td>
        <td><%= link_to (image_tag "pen29.png"), edit_element_path(element) %></td>
        <td><%= link_to (image_tag "delete96.png"), element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Element', new_element_path %>
