<p id="notice"><%= notice %></p>

<h1>Listing Feeders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feeders.each do |feeder| %>
      <tr>
        <td><%= feeder.user.name %></td>
        <td><%= link_to 'Show', feeder %></td>
        <td><%= link_to 'Edit', edit_feeder_path(feeder) %></td>
        <td><%= link_to 'Destroy', feeder, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feeder', new_feeder_path %>
